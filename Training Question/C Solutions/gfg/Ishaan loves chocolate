#include <stdio.h>

int main() 
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n,i,min;
        int a[1000];
        scanf("%d",&n);
        for(i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
        }
        min=a[0];
        for(i=0;i<n;i++)
        {
            if(a[i]<min)
            {
                min=a[i];
            }
        }
        printf("%d\n",min);
    }
	
	
}
